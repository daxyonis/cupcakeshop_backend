{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "product"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "price": {
      "type": "float"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "Custom_field": {
      "type": "component",
      "repeatable": true,
      "component": "custom.custom-field"
    }
  }
}
